{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Hangfire": "Information",
      "HangfireJobsLogger": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Information",
      "Microsoft.AspNetCore.Diagnostics.HealthChecks": "Information",
      "Microsoft.Extensions.Diagnostics.HealthChecks": "Information",
      "AspNetCore.HealthChecks": "Information",
      "HealthChecks": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "None",
        "Hangfire": "None",
        "Microsoft": "None",
        "Microsoft.Hosting.Lifetime": "None",
        "System.Net.Http.HttpClient": "None",
        "Microsoft.AspNetCore.Diagnostics.HealthChecks": "None",
        "Microsoft.Extensions.Diagnostics.HealthChecks": "None",
        "AspNetCore.HealthChecks": "None",
        "HealthChecks": "None"
      },
      "EnableAdaptiveSampling": true
    }
  },
  "Authentication": {
    "ForgotPasswordCallbackUrl": "http://localhost:8080/#/auth/forgot-password",
    "ConfirmEmailCallbackUrl": "http://localhost:8080/#/auth/confirm-email"
  },
  "MySQL": {
    "EnableSensitiveDataLogging": true,
    "EnableDetailedErrors": true
  },
  "Swagger": {
    "Enabled": true,
    "AuthSettings": {
      "RequireAuth": false
    },
    "SwaggerEndpoint": "/swagger/v1/swagger.json",
    "HangfireEndpoint": "http://localhost:5003/hangfire"
  },
  "Hangfire": {
    "Dashboard": {
      "Enabled": true,
      "RequireAuth": false
    }
  },
  "BackgroundJobs": {
    "PullAuctionDataBackgroundJob": {
      "AlwaysProcessCertainItemsEnabled": false
    }
  },
  "SendGrid": {
    "Enabled": false
  }
}
