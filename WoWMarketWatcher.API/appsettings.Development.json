{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "None",
        "Hangfire": "None",
        "Microsoft": "None",
        "Microsoft.Hosting.Lifetime": "None"
      }
    }
  },
  "MySQL": {
    "EnableSensitiveDataLogging": true,
    "EnableDetailedErrors": true
  },
  "Swagger": {
    "Enabled": true,
    "AuthSettings": {
      "RequireAuth": false
    },
    "SwaggerEndpoint": "/swagger/v1/swagger.json",
    "HangfireEndpoint": "http://localhost:5003/hangfire"
  },
  "Hangfire": {
    "Dashboard": {
      "Enabled": true,
      "RequireAuth": false
    }
  }
}
