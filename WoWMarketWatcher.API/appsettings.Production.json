{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Hangfire": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Hangfire": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "MySQL": {
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": true
  },
  "Swagger": {
    "Enabled": true,
    "AuthSettings": {
      "RequireAuth": true
    },
    "SwaggerEndpoint": "../swagger/v1/swagger.json",
    "HangfireEndpoint": "https://rwherber.com/wow-market-watcher/v1/hangfire"
  },
  "Hangfire": {
    "Dashboard": {
      "Enabled": true,
      "RequireAuth": true
    }
  }
}
