{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Hangfire": "Warning",
      "HangfireJobsLogger": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "Microsoft.AspNetCore.Diagnostics.HealthChecks": "Warning",
      "Microsoft.Extensions.Diagnostics.HealthChecks": "Warning",
      "AspNetCore.HealthChecks": "Warning",
      "HealthChecks": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Hangfire": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "Microsoft.AspNetCore.Diagnostics.HealthChecks": "Warning",
        "Microsoft.Extensions.Diagnostics.HealthChecks": "Warning",
        "AspNetCore.HealthChecks": "Warning",
        "HealthChecks": "Warning"
      },
      "EnableAdaptiveSampling": false
    }
  },
  "Authentication": {
    "ForgotPasswordCallbackUrl": "http://localhost:8080/#/forgot-password",
    "ConfirmEmailCallbackUrl": "http://localhost:8080/#/confirm-email"
  },
  "MySQL": {
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": true
  },
  "Swagger": {
    "Enabled": true,
    "AuthSettings": {
      "RequireAuth": true
    },
    "HangfireEndpoint": "https://rwherber.com/wow-market-watcher/hangfire"
  },
  "Hangfire": {
    "Dashboard": {
      "Enabled": true,
      "RequireAuth": true
    }
  },
  "BackgroundJobs": {
    "PullAuctionDataBackgroundJob": {
      "AlwaysProcessCertainItemsEnabled": true
    }
  },
  "SendGrid": {
    "Enabled": false
  }
}
